<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="customer_account_navigation">
            <block class="Magento\Customer\Block\Account\SortLinkInterface" name="boqconfigurator_quote_save" >
                <arguments>
                    <argument name="path" xsi:type="string">boqconfigurator/boq/quote</argument>
                    <argument name="label" xsi:type="string">My Saved Quotes</argument>
                    <argument name="sortOrder" xsi:type="number">240</argument>
                </arguments>
            </block>
        </referenceBlock>
    </body>
</page>
