<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Sales::sales" sortOrder="20">
                    <resource id="Redstage_OrderInstallation::orderinstallation" title="Order Installation" sortOrder="35"/>
                    <resource id="Redstage_OrderInstallation::orderexport" title="Export Order Data" sortOrder="10"/>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
