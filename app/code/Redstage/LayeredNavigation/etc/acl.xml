<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Redstage_Base::main_menu" title="Redstage" sortOrder="100">
                    <resource id="Redstage_LayeredNavigation::layerednavigation" title="Numeric Configurator" translate="title" sortOrder="100">
                        <resource id="Redstage_LayeredNavigation::layerednavigationset" title="Numeric Configurator Setting" translate="title" sortOrder="100"/>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
