"CREATE TABLE `marketplace_product` (\n  `entity_id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'Entity Id',\n  `mageproduct_id` int(10) unsigned NOT NULL DEFAULT 0 COMMENT 'Magento Product Id',\n  `mage_pro_row_id` int(10) unsigned NOT NULL DEFAULT 0 COMMENT 'Magento Product Row Id',\n  `adminassign` int(10) unsigned NOT NULL DEFAULT 0 COMMENT 'Admin Assign Id',\n  `seller_id` int(10) unsigned NOT NULL DEFAULT 0 COMMENT 'Seller Id',\n  `store_id` int(10) unsigned NOT NULL DEFAULT 0 COMMENT 'Store Id',\n  `status` int(10) unsigned NOT NULL DEFAULT 0 COMMENT 'Status',\n  `created_at` timestamp NULL DEFAULT NULL COMMENT 'Creation Time',\n  `updated_at` timestamp NULL DEFAULT NULL COMMENT 'Update Time',\n  `seller_pending_notification` smallint(5) unsigned NOT NULL DEFAULT 0 COMMENT 'Product Notification flag for sellers',\n  `admin_pending_notification` smallint(5) unsigned NOT NULL DEFAULT 0 COMMENT 'Product Notification flag for admin',\n  `is_approved` smallint(5) unsigned NOT NULL DEFAULT 0 COMMENT 'Is product approve by admin for the first time',\n  PRIMARY KEY (`entity_id`),\n  KEY `MARKETPLACE_PRODUCT_MAGE_PRO_ROW_ID` (`mage_pro_row_id`),\n  CONSTRAINT `MARKETPLACE_PRD_MAGE_PRO_ROW_ID_CAT_PRD_ENTT_ROW_ID` FOREIGN KEY (`mage_pro_row_id`) REFERENCES `catalog_product_entity` (`row_id`) ON DELETE CASCADE\n) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='Marketplace Product Table'"