"CREATE TABLE `amasty_file_report` (\n  `item_id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'Item Id',\n  `file_id` int(10) unsigned NOT NULL COMMENT 'File Id',\n  `store_id` smallint(5) unsigned DEFAULT NULL COMMENT 'Store ID',\n  `customer_id` int(10) unsigned DEFAULT NULL COMMENT 'Customer ID',\n  `download_source` smallint(5) unsigned DEFAULT NULL COMMENT 'Download Source',\n  `product_id` int(10) unsigned DEFAULT NULL COMMENT 'Product Id',\n  `category_id` int(10) unsigned DEFAULT NULL COMMENT 'Category Id',\n  `order_id` int(10) unsigned DEFAULT NULL COMMENT 'Order Id',\n  `downloaded_at` timestamp NOT NULL DEFAULT current_timestamp() COMMENT 'Downloaded at',\n  PRIMARY KEY (`item_id`)\n) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='Amasty Product Attachment Report Table'"