"CREATE TABLE `redstage_amc_record` (\n  `id` int(11) NOT NULL AUTO_INCREMENT COMMENT 'Primary Key',\n  `invoice_invoice_no` varchar(255) DEFAULT NULL COMMENT 'Invoice: Invoice No',\n  `invoice_invoice_date` date DEFAULT NULL COMMENT 'Invoice: Invoice Date',\n  `sales_org` varchar(255) DEFAULT NULL COMMENT 'Sales Org',\n  `product_meterial_group_1` varchar(255) DEFAULT NULL COMMENT 'Product: Material Group 1',\n  `product_meterial_group_3` varchar(255) DEFAULT NULL COMMENT 'Product: Meterial Group 3',\n  `asset_name` varchar(255) DEFAULT NULL COMMENT 'Asset Name',\n  `sold_to_account_name` varchar(255) DEFAULT NULL COMMENT 'Sold To: Account Name',\n  `sap_customer_code` varchar(255) DEFAULT NULL COMMENT 'Sap Customer Code',\n  `account_account_name` varchar(255) DEFAULT NULL COMMENT 'Account: Account Name',\n  `account_billing_street` text DEFAULT NULL COMMENT 'Account: Billing Street',\n  `account_city1` varchar(255) DEFAULT NULL COMMENT 'Account: City1',\n  `postal_code` varchar(255) DEFAULT NULL COMMENT 'Postal Code',\n  `account_region` varchar(255) DEFAULT NULL COMMENT 'Account : Region',\n  `equipment_make` varchar(255) DEFAULT NULL COMMENT 'Equipment Make',\n  `amc_contract_number` varchar(255) DEFAULT NULL COMMENT 'AMC Contract Number',\n  `amc_start_date` date DEFAULT NULL COMMENT 'AMC Start Date',\n  `amc_end_date` date DEFAULT NULL COMMENT 'AMC End Date',\n  `contract_net_rate` varchar(255) DEFAULT NULL COMMENT 'Contract Net Rate',\n  `battery_product_product_name` varchar(255) DEFAULT NULL COMMENT 'Battery Product: Product Name',\n  `battery_quantity` int(11) DEFAULT NULL COMMENT 'Battery Quantity',\n  `warranty_start_date` date DEFAULT NULL COMMENT 'Warranty Start Date',\n  `warranty_end_date` date DEFAULT NULL COMMENT 'Warranty End Date',\n  `customer_asset_status` varchar(10) DEFAULT NULL COMMENT 'Customer Asset Status',\n  `product_account_assignment_group` varchar(255) DEFAULT NULL COMMENT 'Product Account: Assignment Group',\n  `batch_id` int(11) NOT NULL COMMENT 'Batch Id ',\n  `status` varchar(20) NOT NULL COMMENT 'Type of Status',\n  `flow_status` varchar(20) NOT NULL COMMENT 'Flow Status',\n  `created_at` timestamp NOT NULL DEFAULT current_timestamp() COMMENT 'Created At',\n  `updated_at` timestamp NOT NULL DEFAULT current_timestamp() COMMENT 'Updated At',\n  PRIMARY KEY (`id`),\n  KEY `REDSTAGE_AMC_RECORD_CUSTOMER_ASSET_STATUS` (`customer_asset_status`)\n) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='Redstage AMC Record'"