"CREATE TABLE `amasty_file` (\n  `file_id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'File Id',\n  `attachment_type` smallint(5) unsigned NOT NULL DEFAULT 0 COMMENT 'Attachment Type',\n  `filepath` varchar(255) NOT NULL DEFAULT '' COMMENT 'File Path',\n  `link` text NOT NULL DEFAULT '' COMMENT 'File Link',\n  `extension` varchar(10) NOT NULL DEFAULT '' COMMENT 'File Extension',\n  `size` int(10) unsigned NOT NULL COMMENT 'File Size',\n  `mime_type` varchar(255) NOT NULL DEFAULT '' COMMENT 'File Mime Type',\n  `url_hash` varchar(32) NOT NULL DEFAULT '' COMMENT 'md5 random hash for url creation',\n  `created_at` timestamp NOT NULL DEFAULT current_timestamp() COMMENT 'Created At',\n  `updated_at` timestamp NOT NULL DEFAULT current_timestamp() ON UPDATE current_timestamp() COMMENT 'Updated At',\n  PRIMARY KEY (`file_id`),\n  KEY `AMASTY_FILE_URL_HASH` (`url_hash`)\n) ENGINE=InnoDB AUTO_INCREMENT=6 DEFAULT CHARSET=utf8 COMMENT='Amasty Product Attachment File Table'"