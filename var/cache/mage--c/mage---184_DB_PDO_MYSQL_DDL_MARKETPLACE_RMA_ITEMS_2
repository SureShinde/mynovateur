"CREATE TABLE `marketplace_rma_items` (\n  `id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'ID',\n  `rma_id` int(11) NOT NULL COMMENT 'RMA Id',\n  `item_id` int(11) NOT NULL COMMENT 'Item Id',\n  `reason_id` int(11) NOT NULL COMMENT 'Reason Id',\n  `product_id` int(11) NOT NULL COMMENT 'Product Id',\n  `qty` int(11) NOT NULL COMMENT 'Quantity',\n  `price` decimal(12,4) NOT NULL DEFAULT 0.0000 COMMENT 'Price',\n  `is_qty_returned` int(11) NOT NULL COMMENT 'Is Quantity Returned',\n  PRIMARY KEY (`id`)\n) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='Marketplace RMA Items Table'"