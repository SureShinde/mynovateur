"CREATE TABLE `amasty_label_tooltip` (\n  `label_id` int(10) unsigned NOT NULL COMMENT 'Label Id',\n  `status` tinyint(3) unsigned NOT NULL DEFAULT 1 COMMENT 'Active Status',\n  `color` varchar(30) DEFAULT NULL COMMENT 'Tooltip Color',\n  `text_color` varchar(30) DEFAULT NULL COMMENT 'Tooltip Text Color',\n  `text` text DEFAULT NULL COMMENT 'Tooltip Text',\n  PRIMARY KEY (`label_id`),\n  CONSTRAINT `AMASTY_LABEL_TOOLTIP_LABEL_ID_AMASTY_LABEL_ENTITY_LABEL_ID` FOREIGN KEY (`label_id`) REFERENCES `amasty_label_entity` (`label_id`) ON DELETE CASCADE\n) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='Label Tooltip Data'"