"CREATE TABLE `amasty_label_entity` (\n  `label_id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'Label Id',\n  `name` varchar(255) NOT NULL COMMENT 'Label Name',\n  `status` tinyint(3) unsigned NOT NULL DEFAULT 0 COMMENT 'Label Status',\n  `priority` tinyint(3) unsigned NOT NULL DEFAULT 0 COMMENT 'Label Priority',\n  `is_single` tinyint(3) unsigned NOT NULL DEFAULT 0 COMMENT 'Hide if Label with Higher Priority is Already Applied',\n  `use_for_parent` tinyint(3) unsigned NOT NULL DEFAULT 0 COMMENT 'Display child`s label for parent (configurable and grouped products only)',\n  `active_from` datetime DEFAULT NULL COMMENT 'Active from',\n  `active_to` datetime DEFAULT NULL COMMENT 'Active to',\n  `conditions_serialized` text NOT NULL COMMENT 'Display Label Conditions',\n  PRIMARY KEY (`label_id`),\n  KEY `AMASTY_LABEL_ENTITY_STATUS` (`status`),\n  KEY `AMASTY_LABEL_ENTITY_STATUS_PRIORITY` (`status`,`priority`)\n) ENGINE=InnoDB AUTO_INCREMENT=4 DEFAULT CHARSET=utf8 COMMENT='Label Main Entity'"