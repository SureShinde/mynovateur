"CREATE TABLE `amasty_menu_item_content` (\n  `id` int(11) NOT NULL AUTO_INCREMENT COMMENT 'Amasty Menu Item Auto ID',\n  `entity_id` int(11) NOT NULL COMMENT 'Amasty Menu Item ID',\n  `type` varchar(20) NOT NULL COMMENT 'Amasty Menu Item Type (category or amasty link)',\n  `store_id` int(11) NOT NULL COMMENT 'Store ID',\n  `name` varchar(255) DEFAULT NULL COMMENT 'Amasty Menu Item Name',\n  `status` smallint(6) DEFAULT 0,\n  `label` varchar(255) DEFAULT NULL COMMENT 'Amasty Menu Label',\n  `label_text_color` varchar(255) DEFAULT NULL COMMENT 'Amasty Menu Label Color',\n  `label_background_color` varchar(255) DEFAULT NULL COMMENT 'Amasty Menu Label Color',\n  `use_default` text DEFAULT NULL COMMENT 'Use Default Fields',\n  `content` mediumtext DEFAULT NULL COMMENT 'Amasty Menu Content',\n  `width` smallint(6) DEFAULT NULL COMMENT 'Amasty Menu Item Width',\n  `width_value` smallint(6) DEFAULT NULL COMMENT 'Amasty Menu Item Width Value',\n  `column_count` smallint(6) DEFAULT NULL COMMENT 'Amasty Menu Item Column Count',\n  `icon` text DEFAULT NULL COMMENT 'Icon',\n  `submenu_type` smallint(6) DEFAULT NULL COMMENT 'Submenu Type',\n  `subcategories_position` smallint(6) DEFAULT NULL COMMENT 'Subcategories Position',\n  `hide_content` smallint(6) DEFAULT NULL COMMENT 'Display Category as Single Menu Item',\n  PRIMARY KEY (`id`),\n  UNIQUE KEY `AMASTY_MENU_ITEM_CONTENT_ENTITY_ID_STORE_ID_TYPE` (`entity_id`,`store_id`,`type`)\n) ENGINE=InnoDB AUTO_INCREMENT=50 DEFAULT CHARSET=utf8 COMMENT='Amasty Mega Menu Link Table'"