"CREATE TABLE `amasty_label_store` (\n  `label_id` int(10) unsigned NOT NULL COMMENT 'Label Id',\n  `store_id` smallint(5) unsigned NOT NULL COMMENT 'Store ID',\n  PRIMARY KEY (`label_id`,`store_id`),\n  KEY `AMASTY_LABEL_STORE_STORE_ID_STORE_STORE_ID` (`store_id`),\n  CONSTRAINT `AMASTY_LABEL_STORE_LABEL_ID_AMASTY_LABEL_ENTITY_LABEL_ID` FOREIGN KEY (`label_id`) REFERENCES `amasty_label_entity` (`label_id`) ON DELETE CASCADE,\n  CONSTRAINT `AMASTY_LABEL_STORE_STORE_ID_STORE_STORE_ID` FOREIGN KEY (`store_id`) REFERENCES `store` (`store_id`) ON DELETE CASCADE\n) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='Label to Store Relation'"