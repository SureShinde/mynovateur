"CREATE TABLE `marketplace_rma_details` (\n  `id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'Id',\n  `order_id` int(11) NOT NULL COMMENT 'Order Id',\n  `seller_id` int(11) NOT NULL COMMENT 'Seller Id',\n  `customer_id` int(11) NOT NULL COMMENT 'Customer Id',\n  `customer_email` varchar(255) NOT NULL COMMENT 'Customer Email',\n  `status` int(11) NOT NULL COMMENT 'Status',\n  `order_status` int(11) NOT NULL COMMENT 'Order Status',\n  `seller_status` int(11) NOT NULL COMMENT 'Seller Status',\n  `final_status` int(11) NOT NULL COMMENT 'Final Status',\n  `product_id` varchar(255) NOT NULL COMMENT 'Product Id',\n  `resolution_type` int(11) NOT NULL COMMENT 'Resolution Type',\n  `number` text NOT NULL,\n  `order_ref` varchar(255) NOT NULL COMMENT 'Order Reference',\n  `additional_info` varchar(255) NOT NULL COMMENT 'Additional Information',\n  `created_date` varchar(255) NOT NULL COMMENT 'Created Date',\n  `refunded_amount` decimal(12,4) NOT NULL COMMENT 'Refunded Amount',\n  `memo_id` int(11) NOT NULL COMMENT 'Credit Memo Id',\n  `customer_name` text NOT NULL COMMENT 'Customer Name',\n  `product_seller` text NOT NULL COMMENT 'Product Seller',\n  `tracking_id` text NOT NULL COMMENT 'tracking id',\n  PRIMARY KEY (`id`)\n) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='Marketplace RMA Details Table'"