"CREATE TABLE `amasty_label_index` (\n  `label_id` int(10) unsigned NOT NULL COMMENT 'Label Id',\n  `product_id` int(10) unsigned NOT NULL COMMENT 'Product Id',\n  `store_id` smallint(5) unsigned NOT NULL COMMENT 'Store ID',\n  PRIMARY KEY (`product_id`,`store_id`,`label_id`),\n  KEY `AMASTY_LABEL_INDEX_LABEL_ID_AMASTY_LABEL_ENTITY_LABEL_ID` (`label_id`),\n  CONSTRAINT `AMASTY_LABEL_INDEX_LABEL_ID_AMASTY_LABEL_ENTITY_LABEL_ID` FOREIGN KEY (`label_id`) REFERENCES `amasty_label_entity` (`label_id`) ON DELETE CASCADE\n) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='Label Index for Increase Frontend Performance'"