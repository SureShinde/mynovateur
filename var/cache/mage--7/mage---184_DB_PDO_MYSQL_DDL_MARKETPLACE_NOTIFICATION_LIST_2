"CREATE TABLE `marketplace_notification_list` (\n  `entity_id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'Entity Id',\n  `notification_id` int(10) unsigned NOT NULL COMMENT 'Notification Id',\n  `notification_row_id` int(10) unsigned NOT NULL DEFAULT 0 COMMENT 'Contains Id of product order transaction as per type ',\n  `type` int(10) unsigned NOT NULL COMMENT 'Notification Type',\n  `created_at` timestamp NULL DEFAULT NULL COMMENT 'Creation Time',\n  `updated_at` timestamp NULL DEFAULT NULL COMMENT 'Update Time',\n  PRIMARY KEY (`entity_id`)\n) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='Marketplace Notification List Table'"