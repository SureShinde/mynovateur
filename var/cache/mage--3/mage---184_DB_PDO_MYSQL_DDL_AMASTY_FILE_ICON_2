"CREATE TABLE `amasty_file_icon` (\n  `icon_id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'Icon Id',\n  `filetype` varchar(255) NOT NULL DEFAULT '' COMMENT 'Type of File',\n  `image` varchar(255) NOT NULL DEFAULT '' COMMENT 'Image name',\n  `is_active` tinyint(1) NOT NULL DEFAULT 1 COMMENT 'Is Active',\n  PRIMARY KEY (`icon_id`)\n) ENGINE=InnoDB AUTO_INCREMENT=10 DEFAULT CHARSET=utf8 COMMENT='Amasty Product Attachment Icon Table'"