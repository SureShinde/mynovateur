"CREATE TABLE `amasty_faq_category` (\n  `category_id` smallint(5) unsigned NOT NULL AUTO_INCREMENT COMMENT 'Category Id',\n  `title` varchar(255) NOT NULL COMMENT 'Title',\n  `position` int(11) NOT NULL DEFAULT 0 COMMENT 'Position',\n  `url_key` varchar(255) NOT NULL COMMENT 'Url Key',\n  `status` tinyint(1) NOT NULL COMMENT 'Status',\n  `meta_title` text DEFAULT NULL COMMENT 'Meta Title',\n  `meta_description` text DEFAULT NULL COMMENT 'Meta Description',\n  `created_at` timestamp NOT NULL DEFAULT current_timestamp() COMMENT 'Created At',\n  `updated_at` timestamp NOT NULL DEFAULT current_timestamp() ON UPDATE current_timestamp() COMMENT 'Updated At',\n  `exclude_sitemap` tinyint(1) NOT NULL DEFAULT 0 COMMENT 'Exclude From Sitemap',\n  `canonical_url` varchar(255) DEFAULT NULL COMMENT 'Canonical Url',\n  `nofollow` tinyint(1) NOT NULL DEFAULT 0 COMMENT 'Is Nofollow category',\n  `noindex` tinyint(1) NOT NULL DEFAULT 0 COMMENT 'Is Noindex category',\n  `description` text NOT NULL DEFAULT '' COMMENT 'Description',\n  `icon` varchar(255) DEFAULT NULL COMMENT 'Icon',\n  `visit_count` int(11) NOT NULL DEFAULT 0 COMMENT 'Visit count',\n  `page_title` varchar(255) DEFAULT NULL COMMENT 'Title on Category Page',\n  PRIMARY KEY (`category_id`),\n  FULLTEXT KEY `AMASTY_FAQ_CATEGORY_TITLE_DESCRIPTION` (`title`,`description`)\n) ENGINE=InnoDB AUTO_INCREMENT=3 DEFAULT CHARSET=utf8 COMMENT='Amasty Faq category table'"