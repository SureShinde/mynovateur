"CREATE TABLE `amasty_faq_question` (\n  `question_id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'Question Id',\n  `title` text NOT NULL COMMENT 'Question text',\n  `short_answer` text DEFAULT NULL COMMENT 'Short answer',\n  `answer` text DEFAULT NULL COMMENT 'Answer',\n  `visibility` smallint(5) unsigned NOT NULL DEFAULT 0 COMMENT 'Visibility',\n  `status` tinyint(1) NOT NULL COMMENT 'Status',\n  `name` varchar(255) DEFAULT NULL COMMENT 'Name',\n  `email` varchar(255) DEFAULT NULL COMMENT 'Email',\n  `position` int(11) NOT NULL DEFAULT 0 COMMENT 'Position',\n  `url_key` varchar(255) DEFAULT NULL COMMENT 'Url Key',\n  `positive_rating` int(11) NOT NULL DEFAULT 0 COMMENT 'Question Positive Rating',\n  `negative_rating` int(11) NOT NULL DEFAULT 0 COMMENT 'Question Negative Rating',\n  `total_rating` int(11) NOT NULL DEFAULT 0 COMMENT 'Total Question Rating',\n  `meta_title` text DEFAULT NULL COMMENT 'Meta Title',\n  `meta_description` text DEFAULT NULL COMMENT 'Meta Description',\n  `meta_robots` varchar(255) DEFAULT NULL COMMENT 'Meta Robots',\n  `created_at` timestamp NOT NULL DEFAULT current_timestamp() COMMENT 'Created At',\n  `updated_at` timestamp NOT NULL DEFAULT current_timestamp() ON UPDATE current_timestamp() COMMENT 'Updated At',\n  `exclude_sitemap` tinyint(1) NOT NULL DEFAULT 0 COMMENT 'Exclude From Sitemap',\n  `canonical_url` varchar(255) DEFAULT NULL COMMENT 'Canonical Url',\n  `nofollow` tinyint(1) NOT NULL DEFAULT 0 COMMENT 'Is Nofollow question',\n  `noindex` tinyint(1) NOT NULL DEFAULT 0 COMMENT 'Is Noindex question',\n  `is_show_full_answer` tinyint(1) DEFAULT 0 COMMENT 'Show Full answer In question list',\n  `asked_from_store` smallint(5) unsigned DEFAULT NULL COMMENT 'Asked From Store ID',\n  `visit_count` int(11) NOT NULL DEFAULT 0 COMMENT 'Visit count',\n  `avg_rating` decimal(10,4) unsigned NOT NULL DEFAULT 0.0000 COMMENT 'Average Question Rating',\n  `avg_total` int(11) NOT NULL DEFAULT 0 COMMENT 'Total Average Ratings Count',\n  PRIMARY KEY (`question_id`),\n  KEY `AMASTY_FAQ_QUESTION_TOTAL_RATING` (`total_rating`),\n  KEY `AMASTY_FAQ_QUESTION_ASKED_FROM_STORE_STORE_STORE_ID` (`asked_from_store`),\n  FULLTEXT KEY `AMASTY_FAQ_QUESTION_TITLE_ANSWER` (`title`,`answer`),\n  FULLTEXT KEY `AMASTY_FAQ_QUESTION_SHORT_ANSWER` (`short_answer`),\n  CONSTRAINT `AMASTY_FAQ_QUESTION_ASKED_FROM_STORE_STORE_STORE_ID` FOREIGN KEY (`asked_from_store`) REFERENCES `store` (`store_id`) ON DELETE SET NULL\n) ENGINE=InnoDB AUTO_INCREMENT=10 DEFAULT CHARSET=utf8 COMMENT='Amasty Faq question table'"