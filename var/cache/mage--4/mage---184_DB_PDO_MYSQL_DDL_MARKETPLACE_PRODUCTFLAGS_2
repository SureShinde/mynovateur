"CREATE TABLE `marketplace_productflags` (\n  `entity_id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'Entity Id',\n  `product_id` int(10) unsigned NOT NULL DEFAULT 0 COMMENT 'Product Id',\n  `name` text DEFAULT NULL COMMENT 'Reporter Name',\n  `email` text DEFAULT NULL COMMENT 'Reporter Email',\n  `reason` text DEFAULT NULL COMMENT 'Flag Reason',\n  `created_at` timestamp NULL DEFAULT NULL COMMENT 'Creation Time',\n  PRIMARY KEY (`entity_id`),\n  KEY `MARKETPLACE_PRODUCTFLAGS_PRODUCT_ID` (`product_id`),\n  CONSTRAINT `MARKETPLACE_PRDFLAGS_PRD_ID_CAT_PRD_ENTT_ENTT_ID` FOREIGN KEY (`product_id`) REFERENCES `catalog_product_entity` (`entity_id`) ON DELETE CASCADE\n) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='Marketplace Seller Flag Table'"