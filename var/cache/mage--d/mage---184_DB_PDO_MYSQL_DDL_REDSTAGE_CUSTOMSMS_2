"CREATE TABLE `redstage_customsms` (\n  `id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'ID',\n  `sender_id` text NOT NULL DEFAULT '' COMMENT 'Title',\n  `destination` text NOT NULL DEFAULT '' COMMENT 'Summary',\n  `message` text NOT NULL DEFAULT '' COMMENT 'Description',\n  `sent_at` datetime NOT NULL COMMENT 'Created At',\n  `status` text NOT NULL COMMENT 'Status',\n  PRIMARY KEY (`id`)\n) ENGINE=InnoDB AUTO_INCREMENT=45 DEFAULT CHARSET=utf8 COMMENT='Redstage SMS Table'"