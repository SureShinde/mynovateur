"CREATE TABLE `marketplace_orders` (\n  `delhivery_ship_status` varchar(255) NOT NULL,\n  `label_printed` tinyint(1) NOT NULL DEFAULT 0,\n  `pickup_id` int(10) unsigned NOT NULL DEFAULT 0,\n  `entity_id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'Entity Id',\n  `order_id` int(10) unsigned NOT NULL DEFAULT 0 COMMENT 'Magento Order Id',\n  `product_ids` varchar(255) DEFAULT NULL COMMENT 'Order Product Ids',\n  `seller_id` int(10) unsigned NOT NULL DEFAULT 0 COMMENT 'Seller Id',\n  `shipment_id` varchar(255) NOT NULL DEFAULT '0' COMMENT 'Shipment Id',\n  `invoice_id` varchar(255) NOT NULL DEFAULT '0' COMMENT 'Invoice Id',\n  `creditmemo_id` varchar(255) NOT NULL DEFAULT '0' COMMENT 'Creditmemo Id',\n  `is_canceled` int(10) unsigned NOT NULL DEFAULT 0 COMMENT 'Canceled Status',\n  `shipping_charges` decimal(12,4) NOT NULL DEFAULT 0.0000 COMMENT 'Shipping Charges',\n  `carrier_name` varchar(255) DEFAULT NULL COMMENT 'Carrier Name',\n  `tracking_number` varchar(255) DEFAULT NULL COMMENT 'Tracking Number',\n  `created_at` timestamp NULL DEFAULT NULL COMMENT 'Creation Time',\n  `updated_at` timestamp NULL DEFAULT NULL COMMENT 'Update Time',\n  `tax_to_seller` smallint(5) unsigned NOT NULL DEFAULT 0 COMMENT 'Tax to seller account flag',\n  `total_tax` decimal(12,4) NOT NULL DEFAULT 0.0000 COMMENT 'Total Tax',\n  `coupon_amount` decimal(12,4) NOT NULL DEFAULT 0.0000 COMMENT 'Coupon Amount',\n  `refunded_coupon_amount` decimal(12,4) NOT NULL DEFAULT 0.0000 COMMENT 'Refunded Coupon Amount',\n  `refunded_shipping_charges` decimal(12,4) NOT NULL DEFAULT 0.0000 COMMENT 'Refunded Shipping Amount',\n  `seller_pending_notification` smallint(5) unsigned NOT NULL DEFAULT 0 COMMENT 'Order Notification flag for sellers',\n  `order_status` varchar(32) NOT NULL DEFAULT 'pending' COMMENT 'Order Status',\n  PRIMARY KEY (`entity_id`),\n  KEY `MARKETPLACE_ORDERS_ORDER_ID` (`order_id`),\n  CONSTRAINT `MARKETPLACE_ORDERS_ORDER_ID_SALES_ORDER_ENTITY_ID` FOREIGN KEY (`order_id`) REFERENCES `sales_order` (`entity_id`) ON DELETE CASCADE\n) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='Marketplace Orders Table'"