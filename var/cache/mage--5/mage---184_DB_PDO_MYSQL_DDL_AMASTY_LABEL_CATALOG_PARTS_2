"CREATE TABLE `amasty_label_catalog_parts` (\n  `label_id` int(10) unsigned NOT NULL COMMENT 'Label Id',\n  `type` tinyint(3) unsigned NOT NULL COMMENT 'Part Type',\n  `label_text` text NOT NULL COMMENT 'Label Text',\n  `image` mediumtext DEFAULT NULL COMMENT 'Label Image',\n  `image_size` varchar(255) DEFAULT NULL COMMENT 'Percent of the product image.',\n  `position` tinyint(3) unsigned NOT NULL COMMENT 'Label Position',\n  `style` mediumtext DEFAULT NULL COMMENT 'Styles',\n  PRIMARY KEY (`label_id`,`type`),\n  KEY `AMASTY_LABEL_CATALOG_PARTS_TYPE` (`type`),\n  CONSTRAINT `AMASTY_LABEL_CATALOG_PARTS_LABEL_ID_AMASTY_LABEL_ENTITY_LABEL_ID` FOREIGN KEY (`label_id`) REFERENCES `amasty_label_entity` (`label_id`) ON DELETE CASCADE\n) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='Label Catalog Part Preferences'"