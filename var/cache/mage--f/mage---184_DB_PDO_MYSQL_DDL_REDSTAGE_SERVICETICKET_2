"CREATE TABLE `redstage_serviceticket` (\n  `entity_id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'Entity Id ',\n  `customer_id` int(11) NOT NULL COMMENT 'Customer Id ',\n  `sf_serviceticket_no` varchar(100) NOT NULL COMMENT 'Sales Force Service Ticket No',\n  `sf_serviceticket_status` varchar(100) NOT NULL COMMENT 'Sales Force Service Ticket Status',\n  `product_serial_number` varchar(100) NOT NULL COMMENT 'Product Serial Number',\n  `invoice_number` varchar(50) NOT NULL COMMENT 'Invoice Number',\n  `order_number` varchar(50) NOT NULL COMMENT 'Order Number',\n  `date_of_invoice` timestamp NOT NULL DEFAULT current_timestamp() COMMENT 'Date of Invoice',\n  `name` varchar(100) NOT NULL COMMENT 'Name',\n  `customer_address` varchar(255) NOT NULL COMMENT 'Address',\n  `installation_site_address` varchar(255) NOT NULL COMMENT 'Installation Site Address',\n  `pincode` varchar(10) NOT NULL COMMENT 'Pincode',\n  `mobile` varchar(15) NOT NULL COMMENT 'Mobile',\n  `email` varchar(50) NOT NULL COMMENT 'Email',\n  `request_type` varchar(50) NOT NULL COMMENT 'Request Type',\n  `images` varchar(255) NOT NULL COMMENT 'Images',\n  `status` varchar(20) NOT NULL COMMENT 'Type of Status',\n  `created_at` timestamp NOT NULL DEFAULT current_timestamp() COMMENT 'Created At',\n  `updated_at` timestamp NOT NULL DEFAULT current_timestamp() COMMENT 'Updated At',\n  `message` text NOT NULL COMMENT 'SF Message',\n  `description` text NOT NULL COMMENT 'Description',\n  `store_id` int(11) NOT NULL COMMENT 'Store Id ',\n  `subject` varchar(255) DEFAULT NULL COMMENT 'Subject',\n  `eCom_ref` varchar(255) DEFAULT NULL COMMENT 'eCom ref',\n  PRIMARY KEY (`entity_id`)\n) ENGINE=InnoDB AUTO_INCREMENT=12 DEFAULT CHARSET=utf8 COMMENT='Service'"