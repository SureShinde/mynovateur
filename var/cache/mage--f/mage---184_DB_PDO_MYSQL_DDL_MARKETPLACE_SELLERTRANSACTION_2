"CREATE TABLE `marketplace_sellertransaction` (\n  `entity_id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'Entity Id',\n  `transaction_id` varchar(255) DEFAULT NULL COMMENT 'Transaction Id',\n  `onlinetr_id` varchar(255) DEFAULT NULL COMMENT 'Online Transaction Id',\n  `seller_id` int(10) unsigned NOT NULL DEFAULT 0 COMMENT 'Seller Id',\n  `transaction_amount` decimal(12,4) NOT NULL DEFAULT 0.0000 COMMENT 'Transaction Amount',\n  `type` varchar(255) DEFAULT NULL COMMENT 'Payment Type',\n  `method` varchar(255) DEFAULT NULL COMMENT 'Payment Method',\n  `custom_note` text DEFAULT NULL COMMENT 'Custom Note',\n  `created_at` timestamp NULL DEFAULT NULL COMMENT 'Creation Time',\n  `updated_at` timestamp NULL DEFAULT NULL COMMENT 'Update Time',\n  `seller_pending_notification` smallint(5) unsigned NOT NULL DEFAULT 0 COMMENT 'Notification flag for sellers',\n  PRIMARY KEY (`entity_id`),\n  KEY `MARKETPLACE_SELLERTRANSACTION_SELLER_ID` (`seller_id`),\n  CONSTRAINT `MARKETPLACE_SELLERTRANSACTION_SELLER_ID_CSTR_ENTT_ENTT_ID` FOREIGN KEY (`seller_id`) REFERENCES `customer_entity` (`entity_id`) ON DELETE CASCADE\n) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='Marketplace Transaction Table'"