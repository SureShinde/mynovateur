"CREATE TABLE `amasty_file_store_category` (\n  `file_store_category_id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'File Store Category ID',\n  `category_id` int(10) unsigned DEFAULT NULL COMMENT 'Category Entity ID',\n  `file_store_id` int(10) unsigned DEFAULT NULL COMMENT 'File Store ID',\n  `file_id` int(10) unsigned NOT NULL COMMENT 'File Id',\n  `store_id` smallint(5) unsigned NOT NULL COMMENT 'Store Id',\n  `filename` varchar(255) DEFAULT NULL COMMENT 'File Name',\n  `label` varchar(255) DEFAULT NULL COMMENT 'Label',\n  `is_visible` tinyint(1) DEFAULT NULL COMMENT 'Is File Visible',\n  `customer_groups` varchar(255) DEFAULT NULL COMMENT 'Is File Visible',\n  `include_in_order` tinyint(1) DEFAULT NULL COMMENT 'Include in Order',\n  `position` int(11) DEFAULT NULL COMMENT 'Position',\n  PRIMARY KEY (`file_store_category_id`),\n  KEY `AMASTY_FILE_STORE_CATEGORY_FILE_ID_AMASTY_FILE_FILE_ID` (`file_id`),\n  CONSTRAINT `AMASTY_FILE_STORE_CATEGORY_FILE_ID_AMASTY_FILE_FILE_ID` FOREIGN KEY (`file_id`) REFERENCES `amasty_file` (`file_id`) ON DELETE CASCADE\n) ENGINE=InnoDB AUTO_INCREMENT=37 DEFAULT CHARSET=utf8 COMMENT='Amasty Product Attachment File Store Category Relation Table'"