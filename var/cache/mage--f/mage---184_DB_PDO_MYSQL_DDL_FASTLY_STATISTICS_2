"CREATE TABLE `fastly_statistics` (\n  `stat_id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'Stat id',\n  `action` varchar(30) NOT NULL COMMENT 'Fastly action',\n  `sent` tinyint(1) NOT NULL DEFAULT 0 COMMENT '1 = Curl req. sent | 0 = Curl req. not sent',\n  `state` tinyint(1) NOT NULL DEFAULT 0 COMMENT '1 = configured | 0 = not_configured',\n  `created_at` datetime DEFAULT NULL COMMENT 'Action date',\n  PRIMARY KEY (`stat_id`)\n) ENGINE=InnoDB AUTO_INCREMENT=2 DEFAULT CHARSET=utf8 COMMENT='fastly_statistics'"