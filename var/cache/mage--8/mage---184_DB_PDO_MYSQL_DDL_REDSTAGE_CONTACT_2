"CREATE TABLE `redstage_contact` (\n  `entity_id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'Entity Id ',\n  `store_id` smallint(5) unsigned NOT NULL DEFAULT 0 COMMENT 'Store Id',\n  `name` varchar(255) NOT NULL COMMENT 'Name',\n  `email` varchar(255) NOT NULL COMMENT 'email',\n  `mobile` varchar(255) NOT NULL COMMENT 'mobile',\n  `state` varchar(255) DEFAULT NULL COMMENT 'State',\n  `city` varchar(255) DEFAULT NULL COMMENT 'City',\n  `pincode` varchar(255) DEFAULT NULL COMMENT 'Pincode',\n  `comment` mediumtext DEFAULT NULL COMMENT 'comment',\n  `created_at` timestamp NOT NULL DEFAULT current_timestamp() COMMENT 'Created At',\n  PRIMARY KEY (`entity_id`)\n) ENGINE=InnoDB AUTO_INCREMENT=2 DEFAULT CHARSET=utf8 COMMENT='Service'"