"CREATE TABLE `redstage_layerednavigation` (\n  `entity_id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'Entity Id ',\n  `title` varchar(255) NOT NULL COMMENT 'Title',\n  `application` varchar(100) NOT NULL COMMENT 'Application',\n  `application_type` varchar(100) NOT NULL COMMENT 'Application Type',\n  `product_attributes` varchar(255) NOT NULL COMMENT 'Product Attributes',\n  `application_attributes` text DEFAULT NULL COMMENT 'Application Attributes',\n  `starting_attribute` varchar(100) NOT NULL COMMENT 'Starting Attribute',\n  `category_apply` varchar(255) NOT NULL COMMENT 'Category Apply',\n  `status` smallint(5) unsigned NOT NULL DEFAULT 0 COMMENT 'Status',\n  `parent` int(11) NOT NULL COMMENT 'Parent',\n  `level` int(11) NOT NULL COMMENT 'Level',\n  `sorting` int(11) NOT NULL COMMENT 'Sorting',\n  `created_at` timestamp NOT NULL DEFAULT current_timestamp() COMMENT 'Created At',\n  `updated_at` timestamp NOT NULL DEFAULT current_timestamp() COMMENT 'Updated At',\n  PRIMARY KEY (`entity_id`)\n) ENGINE=InnoDB AUTO_INCREMENT=4 DEFAULT CHARSET=utf8 COMMENT='Layered Navigation'"